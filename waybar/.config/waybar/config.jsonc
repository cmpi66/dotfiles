{
  "layer": "top", // Waybar at top layer
  "position": "top", // Waybar position (top|bottom|left|right)
  "height": 10, // Waybar height (to be removed for auto height)
  "exclusive": true,
  "passthrough": false,
  "gtk-layer-shell": true,
  // "width": 1280, // Waybar width
  // "spacing": 4, // Gaps between modules (4px)
  "modules-left": [
    "custom/icon",
    "hyprland/workspaces",
    "hyprland/mode",
    "hyprland/scratchpad"
  ],
  "modules-center": [
    "hyprland/window"
  ],
  "modules-right": [
    "custom/mail",
    "custom/torrent",
    "custom/kern",
    "custom/emerge",
    "custom/internet",
    "cpu",
    "memory",
    "temperature",
    "disk",
    "clock",
    "tray",
    "custom/music"
  ],
  // Modules configuration
  // "hyprland/workspaces": {
  //     "disable-scroll": true,
  //     "all-outputs": true,
  //     "on-click": "activate",
  //     "format": "{icon}",
  //     "format-icons": {
  //         "urgent": "ÔÅ™",
